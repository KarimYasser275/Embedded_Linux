/ {
 overlay-name = "Jetson 30pin Header";
 compatible = "nvidia,p2597-0000+p2180-1000";

 fragment@0 {
  target = <&pinmux>;
  __overlay__ {
   pinctrl-names = "default";
   pinctrl-0 = <&jetson_io_pinmux>;
   jetson_io_pinmux: exp-header-pinmux {
    hdr30-pin23 {
     nvidia,pins = "pk3";
     nvidia,function = "i2s5b";
     nvidia,pin-group = "i2s5";
     nvidia,pin-label = "i2s5b_sclk";
     nvidia,pull = <0>;
     nvidia,tristate = <0>;
     nvidia,enable-input = <1>;
    };
    hdr30-pin24 {
     nvidia,pins = "pk2";
     nvidia,function = "i2s5b";
     nvidia,pin-group = "i2s5";
     nvidia,pin-label = "i2s5b_dout";
     nvidia,pull = <0>;
     nvidia,tristate = <0>;
     nvidia,enable-input = <0>;
    };
    hdr30-pin25 {
     nvidia,pins = "pk1";
     nvidia,function = "i2s5b";
     nvidia,pin-group = "i2s5";
     nvidia,pin-label = "i2s5b_din";
     nvidia,pull = <1>;
     nvidia,tristate = <1>;
     nvidia,enable-input = <1>;
    };
    hdr30-pin26 {
     nvidia,pins = "pk0";
     nvidia,function = "i2s5b";
     nvidia,pin-group = "i2s5";
     nvidia,pin-label = "i2s5b_fs";
     nvidia,pull = <0>;
     nvidia,tristate = <0>;
     nvidia,enable-input = <1>;
    };
   };
  };
 };
};
